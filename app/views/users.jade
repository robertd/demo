extends layout
block content
          .demo-charts.mdl-color--white.mdl-shadow--2dp.mdl-cell.mdl-cell--12-col.mdl-grid
            table.mdl-data-table.mdl-js-data-table.mdl-data-table.mdl-shadow--2dp.mdl-cell.mdl-cell--12-col
              thead
                tr
                  th First Name
                  th Last Name
                  th Description
              tbody
                each val, index in JSON.parse(users)
                  tr
                    td= val.firstName
                    td= val.lastName
                    td= val.description